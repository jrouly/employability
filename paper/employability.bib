    @article{ccrsOrganizer2014,
      title={The College and Career Readiness and Success Organizer},
      author={Balestreri, Kathryn and Sambolt, Megan and Duhon, Chad and Smerdon, Becky and Harris, Joseph},
      journal={College and Career Readiness and Success Center},
      year={2014},
      publisher={ERIC},
      url={https://ccrscenter.org/sites/default/files/College%20and%20Career%20Readiness%20and%20Success%20Organizer%20Brief_FINAL.pdf},
      annotation={
        This is a comprehensive handbook exploring pathways to improving overall
        career and college readiness in American students. After the completion
        of secondary education, most students have the choice to pursue a career
        immediately or enter an institution of post-secondary education. A
        student's success in either of these subsequent endeavors is heavily
        predicted by their readiness not only to enter the space, but to decide
        which path to take. The student's education is the best opportunity to
        improve this readiness.

        A great deal of factors in a student's education play into this readiness.
        The CCRS Organizer defines four high level categories each broken down
        into further educational components. Of these many components, ``Academic
        Content'' --- the domain knowledge a student must acquire to succeed in a
        post-secondary field --- is only one.
      }
    }
  }}}

  {{{ Definition of employability
    @article{harvey2000,
      title={An employability performance indicator},
      author={Harvey, Lee},
      journal={Perspectives: Policy \& Practice in Higher Education},
      volume={4},
      number={4},
      pages={105--109},
      year={2000},
      publisher={Taylor \& Francis Group},
      url={https://doi.org/10.1080/13603100050145173},
      annotation={
        Motivates the idea that universities are being viewed as primarily
        preparing students for the workforce. That is, universities should
        be maximizing the 'employability' of their students. Discusses the
        notion of an Employability Performance Indicator (EPI) and its
        merits and consequences. Ultimately, any measure of an EPI should
        not be used for comparisons or 'league tables' to compare institutions,
        as so many factors that cannot be considered within a single EPI would
        be lost by the wayside (including institutional variance, employer
        variance, economic and policy factors, etc). Any EPI should primarily
        be directed to help improve institutions and used internally.

        Employability-development is the idea that institutions help students
        develop their employability --- that is, the things an institution can
        do to help maximize the preparedness of a student for the workforce.

        Ultimately any EPI must be clearly defined in what it measures.
        Many indicators run the risk of at best not clearly measuring anything
        of value, or at worst misleading students, educators, and employers.
      }
    }

    @article{harvey2001,
      title={Defining and measuring employability},
      author={Harvey, Lee},
      journal={Quality in higher education},
      volume={7},
      number={2},
      pages={97--109},
      year={2001},
      publisher={Taylor \& Francis},
      url={https://doi.org/10.1080/13538320120059990},
      annotation={
        Defining employability is extremely difficult. Starting with outcome-based
        definitions (i.e., whether or not the student actually got a job) can be
        tricky:

          ... pragmatic measures prioritise job acquisition as it is easier to
          measure but, in so doing, measure the effectiveness of the institution
          rather than the employability of the graduate.

        Definitions of terms are also critical. Even words like 'graduate' can
        be misconstrued. Any measure of employability must take into account
        areas for improvement. External and comparative measures of instutitions
        (frequently resulting in tables / comparisons / benchmarks / etc) can
        be harmful if definitions are unclear, if the metrics are not self-aware
        and transparent.

        Additionally, what an employer is looking for (e.g. in a job description)
        does not necessarily correlate with a measure of succses in that job.
        Employers are assumed to act 'rationally', but there may be evidence to
        the contrary.
      }
    }

